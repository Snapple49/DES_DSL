grammar des.missionrobot.RobotDSL with org.eclipse.xtext.common.Terminals

generate robotDSL "http://www.missionrobot.des/RobotDSL"

Behaviour:
	'Behavior' name = ID
	'priority' prios = INT
	'movements:' moves = Movement 
;

Movement: MoveForward |	MoveBackward | Turn | Stop
;

MoveForward: 
	'ForwardSpeed:' fspeed = INT
;

MoveBackward: 
	'BackwardSpeed:' bspeed = INT
;

Turn: 
	'TurnAngle:' angle = INT
;

Stop:
	'WaitFor:' duration = INT
;